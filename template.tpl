// Required APIs
const encodeUriComponent  = require('encodeUriComponent');
const copyFromDataLayer   = require('copyFromDataLayer');
const generateRandom      = require('generateRandom');
const getCookieValues     = require('getCookieValues');
const setCookie           = require('setCookie');
const injectHiddenIframe  = require('injectHiddenIframe');
const getTimestamp        = require('getTimestamp');
const getType             = require('getType');
const makeString          = require('makeString');

// constants
const tagVersion = 'GTM-TT-0.1';
const transType  = 'Order';
const s2mHost    = 'https://track.shop2market.com/v3/update_order?fromjs=1&shop_id=';
const cookiesToDelete = [
  "__s2m_vals_order_id_" + (data.hasOwnProperty('shopId') ? data.shopId : ''),
  "__s2m_order_page_sid"
];

//////////////////////////////////////////////////////////////////

if (data.hasOwnProperty('shopId')) {
  track();  
} else {
  data.gtmOnFailure();
}

function track(){
  let  order = (data.ecommerce) ? ecommerce() : manualInput();
  if (getType(order) === 'object') {
    let transaction     = {};
    transaction.shop    = setShop();
    transaction.version = setVersion();
    transaction.info    = setOrderInfo(order.info);
    transaction.items   = setItemInfo(order.items, order.info);
    transact(transaction);
  } else {
    data.gtmOnFailure();
  }
}

//////////////////////////////////////////////////////////////////

function ecommerce() {
  // create order object from dataLayer.ecommerce.purchase object
  
  let orderInfo = copyFromDataLayer('ecommerce', 2);
  if (orderInfo) {
    return {
      info:   orderInfo.purchase.actionField,
      items:  orderInfo.purchase.products
    };
  } else {
    data.gtmOnFailure();
  }
}

function manualInput() {
  // create order object from variables configured in tag fields
  
  if (data.hasOwnProperty("orderInfo") || data.hasOwnProperty("products")) {
    return {
      info: data.orderInfo,
      items: data.products
    };
  } else {
    data.gtmOnFailure();
  }
}

function setShop(){
  // return shopId from user form
    return data.shopId;
}

function setVersion(){
  // return tag version defined in constants
  return tagVersion;
}

function setOrderInfo(orderInfo) {
  // adds order info to transaction object
  
  return {
    trans_id:     orderInfo.id,
    trans_type:   transType,
    amount_1:     makeString(orderInfo.revenue),
    amount_2:     priceExcluding(orderInfo.revenue, orderInfo.tax),
    amount_3:     orderInfo.shipping,
    coupon_code:  orderInfo.coupon
  };
}

function setItemInfo(items, info) {
  // add items to transaction object
  
  let orderItems = [];
  for ( let i in items ){
    if (items[i].id){
      orderItems.push(addItem(items[i], info.id));
    }
  }
  return orderItems;
}

function addItem(item, id){
  // adds one item to transaction object
  
  let orderItem = {};
  orderItem["oi[][trans_id]"]        = id;
  orderItem["oi[][item_id]"]         = item.id;
  orderItem["oi[][quantity"]         = item.quantity;
  orderItem["oi[][item_info_1]"]     = item.name || "";
  orderItem["oi[][item_info_2]"]     = item.brand || "";
  orderItem["oi[][item_info_3]"]     = item.category || "";
  orderItem["oi[][item_amount_1]"]   = "";
  orderItem["oi[][item_amount_2]"]   = "";
  orderItem["oi[][item_amount_3]"]   = "";

  return orderItem;
}

function transact(transaction) {
  // commit transaction

  let s2mVals     = getCookie(cookieKeyShop("__s2m_vals_")) || "";
  let s2mOptin    = getCookie("optin") || "";
  let s2mCsid     = getCookie("session_id") || "";
  let iframepath  = iframePath(transaction, s2mVals, s2mOptin, s2mCsid);
  injectHiddenIframe(iframepath, transactOnSuccess());
}

function transactOnSuccess() {    
  removeS2mCookies();
  data.gtmOnSuccess();
}


  //////////////////////
 // HELPER FUNCTIONS //
/////////////////////

function priceExcluding(revenue, tax) {
  return makeString(revenue - tax);
}

function cookieKeyShop(key) {
  return key + data.shopId;
}

function iframePath(transaction, s2mVals, s2mOptin, s2mCsid) {
  if (data.hasOwnProperty('shopId')){ 
    let path = s2mHost + data.shopId;
    path += "&stamp=" + stamp();
    path += "&tracking_type=0";
    path += "&cookie_uuid=" + encodeUriComponent(s2mVals);
    path += "&s2m_optin=" + encodeUriComponent(s2mOptin);
    path += "&s2m_csid=" + encodeUriComponent(s2mCsid);
    path += encodeObject(transaction.info);
    for (let i in transaction.items) {
      path += encodeObject(transaction.items[i]);
    }
    return path;
  }  
}

function stamp(){
  return generateRandom(0, 99999999) / 100000000;
}

function encodeObject(obj) {
  let str = "";
  for (let key in obj ){
    str += "&" + key + "=" + encodeUriComponent(obj[key]);
  }
  return str;
}

function getCookie(cookieKey) {
  // get cookie values and deduplicates
  
  let values = getCookieValues(cookieKey);
  if (values.length > 1) {
    return arrayMax(values);
  } else {
    return values[0];
  } 
}

function removeS2mCookies(){
  // removes cookies defined in constant cookiesToDelete
  
  for (let i in cookiesToDelete) {
    let key  = cookiesToDelete[i];
    let value = getCookieValues(key);
    if (getType('value') !== 'array' || !value.length) {
      removeCookie(key);
    }
  }
}

function removeCookie(cookieKey) {
  // removes cookie by setting expiry to the past
  
  let options = {expires: "Fri, 02-Jan-1970 00:00:00 GMT"};
  
  setCookie(cookieKeyShop("__s2m_vals_order_id_"), '', options);
  setCookie("__s2m_order_page_sid", '', options);
}

function arrayMax(arr) {
  return arr.reduce(function (a, b) {
    return ( a.length > b.length ? a : b );
  });
}

  //////////////////////////
 // END HELPER FUNCTIONS //
//////////////////////////


